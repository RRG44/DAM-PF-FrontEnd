{
  "expo": {
    "name": "Secure Surf",
    "slug": "Secure Surf",
    "version": "1.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "plugins":[
      [
        "expo-camera", {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "userInterfaceStyle": "automatic",
      "icon": "./assets/images/logoApp.png"
    }
  }
}
